{
  "name": "auth",
  "version": "1.0.0",
  "description": "Authentication Server with Node & Grpc",
  "main": "build/server.js",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "start:dev": "npm run db:generate && npm run db:push && nodemon",
    "nodemon": "nodemon",
    "test": "jest",
    "db:generate": "npx prisma generate --schema ./src/prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev --name user-entity --create-only --schema ./src/prisma/schema.prisma",
    "db:push": "npx prisma db push --schema ./src/prisma/schema.prisma"
  },
  "author": "Javier Ortiz",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.8.1",
    "@grpc/proto-loader": "^0.7.4",
    "@prisma/client": "^4.8.1",
    "add": "^2.0.6",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "prisma": "^4.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.4"
  }
}
